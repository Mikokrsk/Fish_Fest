.AnswerText {
    -unity-font-style: bold;
    font-size: 35px;
    -unity-text-align: upper-left;
    background-color: rgba(0, 0, 0, 0);
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
}

.AnswerText:hover {
    color: rgb(255, 0, 0);
    font-size: 35px;
    -unity-font-style: bold;
}
